{
  "name": "Reroute",
  "version": "0.1.0",
  "description": "Redirect all connections to a route to another.",
  "manifest_version": 2,
  "background": {
    "scripts": [ "./background/scripts/script.js" ],
    "persistent": true
  },
  "icons": {
    "16": "./icons/16.png",
    "32": "./icons/32.png",
    "48": "./icons/48.png",
    "128": "./icons/128.png"
  },
  "page_action": {
    "default_icon": {
      "16": "./page_actions/default_icons/16.png",
      "32": "./page_actions/default_icons/32.png",
      "48": "./page_actions/default_icons/48.png",
      "128": "./page_actions/default_icons/128.png"
    },
    "default_popup": "./page_actions/default_popup/default_popup.html"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "declarativeContent",
    "storage",
    "webRequest",
    "webRequestBlocking"
  ]
}
